<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
  	 <class name="model.Subscription" table="subscription">
 	<id name="subscriptionId" column="subscriptionId" type="int">
 		<generator class="native"/>
 	</id>
 	<property name="userId" column="userId" type="int"></property>
 	<property name="forumId" type="int" column="forumId"></property>
 	</class>	
 </hibernate-mapping>