<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
     <class name="com.campusTalk.model.Domain" table="domain">
    <id name="domainId" column="domainId" type="int">
        <generator class="native"/>
    </id>
    <property name="domainDescription" column="description" type="string"></property>
    </class>    
 </hibernate-mapping>